      <code>#template('colorThemeInit.vm')

## Remove this after it's implemented by default in the base skin
*::-moz-focus-inner {
  padding: 0;
  border: 0
}

#body {
  font-family: Arial,Verdana,sans-serif;
}
.edit_section {
  display: none !important;
}
#headerglobal {
  position: relative;
}
#company h1 a {
  color: $theme.pageBackgroundColor !important;
  text-decoration: none !important;
}
#company h1 {
  border: 0 none;
  color: $theme.pageBackgroundColor;
  font-size: 1.2em;
  margin: 0 20px 0;
  position: absolute;
  right: 5%;
  text-shadow: 0 0 6px #777;
  text-transform: uppercase;
}
.xdocCreation {
  float: right;
  width: 48%;
}
#document-title h1 {
  margin: 0 0 0.5em;
  padding: 5px 0 0;
}
#mainContentArea, #mainEditArea {
  overflow: visible;
  padding: 10px;
  position: relative;
}

.actionmenu {
  height: 1.8em;
}
#phenotips-mainmenu, #phenotips-globalTools {
  background-color: rgba(255, 255, 255, 0.1);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0.2);
  color: $theme.menuLinkColor;
  text-shadow: 0 1px 1px rgba(0, 0, 0, 0.5)
}
#phenotips-mainmenu .topmenuentry {
  background-image: none;
  white-space: nowrap;
}
#phenotips-mainmenu a, #phenotips-globalTools a {
  display: inline-block;
  float: none;
}
#phenotips-globalTools .search input {
  height: 1.7em;
  margin-left: 0;
  margin-right: 5px;
  margin-top: 0.05em;
  min-height: 22px;
  padding: 0 3px;
  width: 20em;
}

#phenotips-globalTools label {
  margin-left: 2em;
  white-space: nowrap;
}

#create-patient-record {
  box-sizing: content-box;
  background: transparent;
  border: 0 none;
  color: $theme.menuLinkColor;
  cursor: pointer;
  display: inline-block;
  font: 700 0.8em/1.8em sans-serif;
  height: 1.8em;
  padding: 2px 0 0;
  text-shadow: 0 1px 1px rgba(0, 0, 0, 0.5);
  text-transform: uppercase;
  white-space: nowrap;
  width: auto;
}
#create-patient-record:hover {
  text-decoration: underline;
}
#phenotips-globalTools .search .icon-search, #phenotips-globalTools .search .fa-search {
  color: $theme.menuLinkColor;
  display: inline-block;
  margin-right: 5px;
}

.bottombuttons {
  background: #F9F9F9;
  border: 1px solid #FFFFFF;
  bottom: 0;
  box-shadow: 0 0 8px rgba(0, 0, 0, 0.2);
  left: 0;
  margin: 0 25px !important;
  padding: 0.5em 10px;
  position: fixed;
  right: 0;
  z-index: 3;
}
.bottombuttons input[name="action_cancel"] {
  float: right;
}

input.button:disabled, .buttonwrapper button:disabled, input.button:disabled:hover, .buttonwrapper button:disabled:hover {
    background-image: none;
}

.add-data-button,
.buttonwrapper a.add-data-button:link,
.buttonwrapper a.add-data-button:visited,
.buttonwrapper a.add-data-button:active {
  background-color: $theme.buttonPrimaryBackgroundColor;
  border-radius: 4px;
  box-shadow: 0 0 6px $theme.borderColor;
  color: $theme.buttonPrimaryTextColor;
  display: inline-block;
  font: normal normal 700 .8em/140% "Helvetica",sans-serif;
  padding: .3em .2em .3em  2.2em;
  position: relative;
  text-transform: uppercase;
  white-space: nowrap;
}
.add-data-button:hover,
.buttonwrapper a.add-data-button:hover {
  box-shadow: 0 0 6px $theme.linkColor;
}
.general-tools {
  float: right;
  margin: -1.6em 3px 0;
  padding: 3px 0;
  position: relative;
}
.general-tools label {
  color: $theme.linkColor;
  cursor: pointer;
}
.general-tools label:hover {
  text-decoration: underline;
}
#patient-creation-form {
  position: relative;
}
.xwiki-livetable-pagination #patient-creation-form {
  float: left;
}
label.create-button-label, .add-data-button:before {
  color: $theme.buttonPrimaryTextColor;
  cursor: pointer;
  display: inline-block;
  font-size: 1.25em;
  font-weight: bold;
  height: .8em;
  left: .4em;
  line-height: .9em;
  position: absolute;
  text-align: center;
  text-shadow: none;
  top: .24em;
  vertical-align: middle;
  width: .8em !important;
  z-index: 2;
}

thead.xwiki-livetable-display-header td.xwiki-livetable-display-header-filter input, thead.xwiki-livetable-display-header td.xwiki-livetable-display-header-filter select {
  padding: .3em !important;
}

input[type=text], input[type=password], textarea {
  border-color: $theme.borderColor;
  border-radius: 4px;
  box-shadow: 0 1px 2px rgba(0,0,0,.4) inset !important;
  width: 100%;
}
textarea {
  box-sizing: border-box;
  width: 100%;
}
input[type=text]:focus, input[type=password]:focus, textarea:focus {
  border-color: $theme.linkColor;
}

body .xwiki-livetable-topfilters-container, body .xwiki-livetable-tagcloud-container {
  background-color: $theme.backgroundSecondaryColor;
  border-color: $theme.buttonPrimaryBackgroundColor;
}
body .xwiki-livetable-topfilters-tip .tippointer, body .xwiki-livetable-tagcloud-tip .tippointer {
  border-top-color: $theme.buttonPrimaryBackgroundColor;
}
body .xwiki-livetable-topfilters-tip .tippointer div, body .xwiki-livetable-tagcloud-tip .tippointer div {
  border-top-color: $theme.backgroundSecondaryColor;
  top: -22px;
}
body .xwiki-livetable-topfilters-container .value-cloud-tools a {
  color: $theme.textColor;
  font-weight: bold;
}
#patients.xwiki-livetable .actions {
  text-align: right;
}
h1:target, h2:target, h3:target {
  background-color: $theme.highlightColor;
}
#globallinks {
  display: none;
}

## Disable gradients
#mainmenu {
  background-image: none;
  box-shadow: none;
}
#mainmenu * {
  text-shadow: none;
}
.editbody #footerglobal {
  margin-bottom: 4em;
}
.main {
  padding-bottom: 0;
}
.gadget label[for=create-patient-record] {
  color: $theme.linkColor;
  cursor: pointer;
  float: right;
}
#xdocFooter {
  display: none;
}</code>
