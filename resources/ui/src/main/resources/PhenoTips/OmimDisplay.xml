<xwikidoc version="1.1">
  <web>PhenoTips</web>
  <name>OmimDisplay</name>
  <language/>
  <defaultLanguage/>
  <translation>0</translation>
  <creator>xwiki:XWiki.Admin</creator>
  <creationDate>1401822206000</creationDate>
  <parent>PhenoTips.WebHome</parent>
